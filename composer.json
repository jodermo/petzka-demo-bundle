{
  "name": "petzka/demo-bundle",
  "type": "contao-bundle",
  "description": "Demo Bundle",
  "homepage": "https://petzka.com",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "Moritz Petzka",
      "homepage": "https://petzka.com",
      "role":"Developer"
    }
  ],
  "require":{
    "php":">=5.5",
    "contao/core-bundle":"^4.4"
  },
  "conflict": {
    "contao/core": "*",
    "contao/manager-plugin": "<2.0 || >=3.0"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.0",
    "doctrine/doctrine-cache-bundle": "^1.3",
    "friendsofphp/php-cs-fixer": "^2.12",
    "php-http/guzzle6-adapter": "^1.1",
    "php-http/message-factory": "^1.0.2",
    "phpunit/phpunit": "^5.7.26",
    "symfony/phpunit-bridge": "^3.2"
  },
  "extra": {
    "contao-manager-plugin": "Petzka\\DemoBundle\\ContaoManager\\Plugin"
  },
  "autoload": {
    "psr-4": {
      "Petzka\\DemoBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Petzka\\DemoBundle\\Tests\\": "tests/"
    }
  },
  "support":{
    "issues":"https://github.com/jodermo/petzka-demo-bundle/issues",
    "source":"https://github.com/jodermo/petzka-demo-bundle"
  },
  "replace": {
    "contao-legacy/demo-bundle": "self.version"
  }
}
