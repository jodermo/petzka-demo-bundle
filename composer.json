{
    "name": "petzka/demo-bundle",
    "type": "contao-bundle",
    "description": "Demo Bundle",
    "homepage": "https://petzka.com",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Moritz Petzka",
            "homepage": "https://petzka.com"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "contao/core-bundle": "*",
        "symfony/framework-bundle": "*"
    },
    "conflict": {
        "contao/core": "*"
    },
    "require-dev": {
        "contao/manager-plugin": "*",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "friendsofphp/php-cs-fixer": "^2.12",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/message-factory": "^1.0.2",
        "phpunit/phpunit": "^5.7.26",
        "symfony/phpunit-bridge": "^3.2"
    },
    "extra": {
        "contao-manager-plugin": "Petzka\\DemoBundle\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "Petzka\\DemoBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Petzka\\DemoBundle\\Tests\\": "tests/"
        }
    },
    "support": {
        "issues": "https://petzka.com",
        "source": "https://petzka.com"
    }
}
